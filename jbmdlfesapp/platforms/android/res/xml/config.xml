<?xml version='1.0' encoding='utf-8'?>
<widget id="net.mobilewebapp.jbmdlfesapp" version="0.5.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>JBMDLFesApp</name>
    <description>
        A sample Apache Cordova application that provides mapping info for JBMDL FES
    </description>
    <author email="aellison@mobilewebapp.net" href="http://mobilewebapp.net">
        Anthony Ellison
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <hook src="../ngbuildprod.js" type="before_prepare" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="./icons/icon-36x36.png" />
    <icon density="mdpi" src="./icons/icon-48x48.png" />
    <icon density="hdpi" src="./icons/icon-72x72.png" />
    <icon density="xhdpi" src="./icons/icon-96x96.png" />
    <icon density="xxhdpi" src="./icons/icon-144x144.png" />
    <icon density="xxxhdpi" src="./icons/icon-192x192.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
